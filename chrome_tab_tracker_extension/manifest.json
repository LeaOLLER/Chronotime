{
  "manifest_version": 3,
  "name": "Tab Tracker",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "ws://localhost:*/*",
    "http://localhost:*/*",
    "*://*.discord.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
